{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        // "react",
        "babel"
    ],
    "rules": {
        "react/prop-types": [
            0
        ],
        "react/jsx-key": [
            0
        ],
        "no-unused-vars": [
            "warn",
            {
                "caughtErrorsIgnorePattern": "^ignore"
            }
        ],
        "react/jsx-uses-react": 1,
        "react/jsx-uses-vars": "warn",
        "linebreak-style": [
            "warn",
            "unix"
        ],
        "babel/new-cap": 1,
        "babel/camelcase": 1,
        "babel/no-invalid-this": 1,
        "babel/object-curly-spacing": 1,
        "babel/semi": 1,
        "babel/no-unused-expressions": 1,
        "babel/valid-typeof": 1
    }
}